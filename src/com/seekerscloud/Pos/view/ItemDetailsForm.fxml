<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="itemDetailsContext" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.seekerscloud.Pos.controller.ItemDetailsFormController">
   <children>
      <JFXButton layoutX="16.0" layoutY="14.0" onAction="#backToHomeOnAction" style="-fx-background-color: #34495e;" text="Back to Home" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <TableView fx:id="tblItems" layoutX="16.0" layoutY="58.0" prefHeight="325.0" prefWidth="565.0">
        <columns>
          <TableColumn fx:id="colCode" prefWidth="129.0" text="Item Code" />
          <TableColumn fx:id="colUnitPrice" prefWidth="175.0" text="Unit Price" />
            <TableColumn fx:id="colQty" prefWidth="136.0" text="Qty" />
            <TableColumn fx:id="colTotal" prefWidth="124.0" text="Total" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
